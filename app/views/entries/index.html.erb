<h1>Listing entries</h1>

<table>
  <tr>
    <th>Feed</th>
    <th>Title</th>
    <th>Author</th>
    <th>Link</th>
    <th>Content</th>
    <th>Published at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @entries.each do |entry| %>
  <tr>
    <td><%= entry.feed_id %></td>
    <td><%= entry.title %></td>
    <td><%= entry.author %></td>
    <td><%= entry.link %></td>
    <td><%= entry.content %></td>
    <td><%= entry.published_at %></td>
    <td><%= link_to 'Show', entry %></td>
    <td><%= link_to 'Edit', edit_entry_path(entry) %></td>
    <td><%= link_to 'Destroy', entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Entry', new_entry_path %>
